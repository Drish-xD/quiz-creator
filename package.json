{
  "name": "quizcreator",
  "version": "0.2.0",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-staging": "cross-env NODE_ENV=staging next build",
    "start:staging": "cross-env NODE_ENV=staging next start",
    "lint": "next lint",
    "format": "prettier --write . --ignore-path .gitignore",
    "test:jest": "jest --ci",
    "test:jest-watch": "jest --watchAll",
    "dev:server": "json-server --watch sessions_db.json --port 3001",
    "test:cypress": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "next/core-web-vitals",
      "next"
    ],
    "ignorePatterns": [
      "**/*.test.*",
      "**/*.cy.*"
    ],
    "rules": {
      "no-undef": "off",
      "react-hooks/exhaustive-deps": "off",
      "no-unused-vars": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "aws-sdk": "^2.1598.0",
    "axios": "^1.6.8",
    "next": "14.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.51.3",
    "react-paginate": "^8.2.0",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.3",
    "eslint": "8.44.0",
    "eslint-config-next": "14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^1.0.0-alpha.23",
    "postcss": "^8.4.38",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
